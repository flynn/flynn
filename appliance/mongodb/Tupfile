include_rules
: |> !go ./cmd/flynn-mongodb |> bin/flynn-mongodb
: |> !go ./cmd/flynn-mongodb-api |> bin/flynn-mongodb-api

: img/packages.sh |> !build-layer |> layer/packages.json
: img/files.sh bin/* start.sh dump.sh restore.sh |> !build-layer |> layer/files.json
: layer/packages.json layer/files.json |> !build-artifact |>
