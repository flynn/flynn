include_rules
: |> !go |> bin/docker-receive
: |> !go ./artifact |> bin/docker-artifact
: |> !go ./migrator |> bin/docker-migrator
: $(ROOT)/util/ca-certs/ca-certs.pem |> !cp |> bin/ca-certs.pem

: img/packages.sh |> !build-layer |> layer/packages.json
: img/files.sh bin/* |> !build-layer |> layer/files.json
: layer/packages.json layer/files.json |> !build-artifact |>
